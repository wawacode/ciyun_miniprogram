/*  pages/register/register.wxss */
.container{
  padding:20rpx;
}
.box_title{
  width:94%;
  height:200rpx;
  background:#76B400;
  padding-left:3%;
  padding-right:3%
}
.title{
  width: 100%;
  height:100rpx;
  line-height: 150rpx;
  color:#fff;
  text-align: center;
}
.box_title image{
  width:150rpx;
  height:150rpx;
  border-radius: 50%;
  margin-top:25rpx;
  margin-left: 40%
}

.login{
  width:100%;
  background:#76B400;
  color:#fff;
  margin-top:29rpx;
}
.container_list{
  width:100%;
  height:80rpx;
  border-bottom: 1rpx solid #ccc;
}
.container_left{
  display: block;
  float: left;
  width:30%;
  height:100%;
  line-height:80rpx; 
  text-align: right
}
.container_right{
  display: block;
  float: right;
  width:68%;
  height:100%;
  line-height: 80rpx;
}
.container_right input{
  width: 98%;
  height:78rpx;
  padding-left:2%
}