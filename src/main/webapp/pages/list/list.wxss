/* pages/list/list.wxss */
Page{
  background:#f3f5f6;
}
.container{
  padding:24rpx;
  
  padding-top:0;
}
.list_box{
  width:100%;
  height:auto;
  margin-top:20px;
}

.list_centent{
  width:644rpx;
  height:220rpx;
  position: relative;
  padding:29rpx;
  background: #fff;
  border-radius: 5rpx;
}
.list_border{
  position: absolute;
  left:80rpx;
  top:-43rpx;
  border:3rpx solid #CBCBCB;
  height:20px;
}
.list_left{
  display:block;
  float: left;
  width:30%;
  height:100%;
}
.list_right{
  display:block;
  width:98%;
  height:100%;
  margin:0 auto;
  position: relative;
}
.list_title{
  width: 100%;
  height:58rpx;
  line-height: 58rpx;
  color:#000;
  font-size:28rpx;
}
.list_mechanism{
  width:100%;
  height:56rpx;
  line-height: 56rpx;
  color:#A6A6A6;
  padding-bottom:22rpx;
  border-bottom:1px solid #CBCBCB;
  font-size: 26rpx;
}
.list_times{
  width:100%;
  height:70rpx;
  line-height: 70rpx;
  color:#A6A6A6;
}
.list_times_left{
  float: left;
  line-height: 110rpx;
  font-size: 22rpx;
}
.list_times_right{
  float:right;
  line-height: 110rpx;
  font-size:22rpx;
}
.list_details{
  width:9%;
  height:120%;
  position: absolute;
  top:-20rpx;
  right:0
}
.btn_box{
  width:100%;
  height:auto;
  overflow: hidden;
  padding-top:10rpx;
  padding-bottom:10rpx;
  background: rgba(239, 239, 239, .8);
  position: fixed;
  bottom: 0;
  left:0;
}
.btn{
  width:40%;
  color:#fff;
  background:#76B400;
}