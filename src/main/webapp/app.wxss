/**app.wxss**/
.container {
  height: 100%;
}
view,scroll-view,text,textarea{
  box-sizing: border-box;
  font-size: 28rpx;
  color: #3d4145;
  font-family:"Helvetica Neue", Helvetica, STHeiTi,"Microsoft YaHei",微软雅黑,"Apple LiGothic Medium",\5b8b\4f53,SimSun,STSong,tahoma,arial,simsun,sans-serif;
  padding: 0;
  margin: 0;
}
input,::after,::before{
  box-sizing: border-box;
}
.clear{
  clear: both;
}
.floatL{
  float: left;
}
.floatR{
  float: right;
}
.fixed{
  position: fixed;
  z-index: 11;
}
.none{
  display: none;
}
.fixed:after{
  overflow: hidden;
  clear: both;
}
.clearfix:after {
    clear: both;
}
.clearfix:before, .clearfix:after {
    content: "";
    display: table;
}

